    "redirects": [
        {
            "source": "/main-concepts/*",
            "destination": "/"
        },
        {
            "source": "/security/oidc",
            "destination": "/how-to-guides/sso"
        },
        {
            "source": "/security/one-login",
            "destination": "/how-to-guides/sso"
        },
        {
            "source": "/security/keycloak",
            "destination": "/how-to-guides/sso"
        },
        {
            "source": "/security/saml*",
            "destination": "/how-to-guides/sso"
        },
        {
            "source": "/security/*",
            "destination": "/how-to-guides/sso/*"
        },
        {
            "source": "/collateai/*",
            "destination": "/collate-ai/*"
        },
        {
            "source": "/how-to-guides/mcp/*",
            "destination": "/collate-ai/mcp/*"
        }
    ],
    "integrations": {
